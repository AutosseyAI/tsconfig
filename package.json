{
  "name": "@autossey/tsconfig",
  "version": "1.1.1",
  "description": "A collection of base TSConfigs for various types of projects.",
  "author": "Connor White",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AutosseyAI/tsconfig.git"
  },
  "keywords": [
    "typescript",
    "tsconfig",
    "tsconfig.json",
    "base"
  ],
  "files": [
    "bun/tsconfig.json",
    "dom/tsconfig.json",
    "next/tsconfig.json",
    "node/tsconfig.json",
    "node-18/tsconfig.json",
    "node-18-cjs/tsconfig.json",
    "tsconfig.json"
  ],
  "scripts": {
    "lint": "find . -name 'tsconfig.json' -exec bun run jsonlint --sort-keys --in-place {} \\;",
    "release": "yarn version"
  },
  "dependencies": {
    "@types/node": "^20.6.4",
    "bun-types": "^1.0.3"
  },
  "devDependencies": {
    "jsonlint": "^1.6.3"
  }
}
